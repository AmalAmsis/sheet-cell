      <VBox alignment="TOP_CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Command" textAlignment="CENTER" VBox.vgrow="ALWAYS">
               <font>
                  <Font name="Blackadder ITC" size="22.0" />
               </font>
            </Label>
            <Accordion prefHeight="283.0">
               <panes>
                  <TitledPane animated="false" prefHeight="199.0" prefWidth="191.0" text="Custom Sort">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="174.0" prefWidth="206.0">
                           <children>
                              <TextField fx:id="sortToTextField" layoutX="109.0" layoutY="27.0" prefHeight="26.0" prefWidth="38.0" promptText="to" styleClass="style-label" />
                              <TextField fx:id="sortFromTextField" layoutX="54.0" layoutY="27.0" prefHeight="27.0" prefWidth="39.0" promptText="from" styleClass="style-label" />
                              <Label layoutX="97.0" layoutY="23.0" prefHeight="36.0" prefWidth="7.0" text="-">
                                 <font>
                                    <Font size="25.0" />
                                 </font>
                              </Label>
                              <Button fx:id="sortButton" layoutX="20.0" layoutY="120.0" mnemonicParsing="false" onAction="#ClickMeSortButton" styleClass="style-button" text="Sort" />
