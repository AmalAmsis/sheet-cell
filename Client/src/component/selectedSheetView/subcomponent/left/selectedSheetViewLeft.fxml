                              <Button fx:id="showRangeButton" layoutX="70.0" layoutY="66.0" mnemonicParsing="false" onAction="#ClickMeShowRange" styleClass="style-button" text="show" />
                           </children>
                        </AnchorPane>
                     </content>
                     <font>
                        <Font name="Bell MT Italic" size="14.0" />
                     </font>
                  </TitledPane>
                  <TitledPane animated="false" text="delete range">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <ChoiceBox fx:id="removeRangeChoiceBox" layoutX="35.0" layoutY="25.0" prefHeight="26.0" prefWidth="121.0" />
